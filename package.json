{
  "name": "app-component",
  "version": "1.0.0",
  "description": "creating a react component App",
  "main": "index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "pnpm run clean-public && rollup -c rollup.prod.config.js",
    "dev": "pnpm run clean-dev && rollup -c rollup.dev.config.js -w",
    "lint": "run-p --continue-on-error lint:*",
    "lint:eslint": "eslint",
    "lint:css": "stylelint **/*.css --ignore-path .gitignore",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean-dev": "rm -rf build",
    "clean-public": "rm -rf public",
    "clean": "rm -rf build public"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.34.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@stylistic/stylelint-config": "^3.0.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "eslint": "^9.34.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "rollup": "^4.50.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-generate-html-template": "^1.7.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^3.0.0",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-prettier": "^5.0.3"
  },
  "dependencies": {
    "prop-types": "^15.8.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
